{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "pnpm run build",
        "nixpacksPlan": {
            "phases": {
                "install": {
                    "dependsOn": [
                        "setup"
                    ],
                    "cmds": [
                        "npm i pnpm && pnpm install"
                    ]
                }
            }
        }
    },
    "deploy": {
        
        "startCommand": "pnpm run start"
       
        
    }
    
    
}